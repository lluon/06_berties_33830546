<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Audit Log â€” <%= shopData.shopName %></title>
  <link rel="stylesheet" href="/main.css">
</head>
<body>

  <h1>Audit Log</h1>

  <table>
    <tr>
      <th>Timestamp</th>
      <th>Username</th>
      <th>Success</th>
      <th>Detail</th>
    </tr>

    <% logs.forEach(log => { %>
      <tr>
        <td><%= log.timestamp %></td>
        <td><%= log.username %></td>
        <td><%= log.success ? "Success" : "Failure" %></td>
        <td><%= log.detail %></td>
      </tr>
    <% }) %>
  </table>

  <p><a href="/">Back to home</a></p>

</body>
</html>
