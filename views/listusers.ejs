<!doctype html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>User List â€” <%= shopData.shopName %></title>
  <link rel="stylesheet" href="/main.css">
</head>
<body>

  <h1>Registered Users</h1>

  <table>
    <tr>
      <th>Username</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Email</th>
    </tr>

    <% users.forEach(user => { %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.first_name %></td>
        <td><%= user.last_name %></td>
        <td><%= user.email %></td>
      </tr>
    <% }) %>
  </table>

  <p><a href="/">Back to home</a></p>

</body>
</html>
